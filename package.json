{
  "name": "tag-n-trac-starter-project",
  "version": "1.0.0-alpha",
  "description": "Tag-N-Trac Starter Project for developing API's using ExpressJS",
  "main": "src/index.js",
  "author": {
    "name": "Vinay Uttam Vemparala",
    "email": "vinay.vemparala@tagntrac.com"
  },
  "engines": {
    "node": "20.18.0"
  },
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "prettier": "prettier --write 'src/**/**.ts'",
    "lint": "eslint src",
    "start:dev": "rimraf dist && tsc && nodemon -r dotenv/config -q -w dist dist/index.js",
    "start": "node -r dotenv/config -q -w dist dist/index.js"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.1",
    "@google-cloud/vertexai": "^1.9.0",
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-http-context": "^1.2.4",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "luxon": "^3.5.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.19.0",
    "@eslint/object-schema": "^2.1.4",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.4.2",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write 'src/**/**.ts'",
      "eslint src"
    ]
  }
}
