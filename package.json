{
  "name": "tag-n-trac-starter-project",
  "version": "1.0.0-alpha",
  "description": "Tag-N-Trac Starter Project for developing API's using ExpressJS",
  "main": "src/index.js",
  "author": {
    "name": "Vinay Uttam Vemparala",
    "email": "vinay.vemparala@tagntrac.com"
  },
  "engines": {
    "node": "20.18.0"
  },
  "private": true,
  "scripts": {
    "build": "rimraf -d dist && tsc",
    "prettier": "prettier --write 'src/**/**.ts'",
    "lint": "eslint src",
    "start:dev": "rimraf -d dist && tsc && nodemon -r dotenv/config -q -w dist dist/index.js",
    "start": "node -r dotenv/config -q -w dist dist/index.js"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.33.0",
    "@google-cloud/bigquery": "^7.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "luxon": "^3.2.1",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/helmet": "^4.0.0",
    "@types/luxon": "^3.2.0",
    "@types/morgan": "^1.9.4",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write 'src/**/**.ts'",
      "eslint src"
    ]
  }
}
